<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @task.title %>
</p>

<p>
  <strong>Content:</strong>
  <%= @task.content %>
</p>

<p>
  <strong>TimeLimit:</strong>
  <%= @task.time_limit %>
</p>

<p>
  <strong>状態:</strong>
  <%= @task.completed %>
</p>

<p>
  <strong>優先順位:</strong>
  <%= @task.priority %>
</p>

<strong>ラベル</strong>
<ul>
<% @task.labels.each do |label| %>
  <li><%= label.name %></li>
<% end %>
</ul>

<%= link_to '編集', edit_task_path(@task) %> |
<%= link_to '戻る', :back %>
